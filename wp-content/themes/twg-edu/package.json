{
  "name": "Departika-Boilerplate",
  "version": "5.0.0",
  "description": "Departika boilerplate with Parcel JS",
  "license": "GPL-2.0 AND MIT",
  "devDependencies": {
    "@juggle/resize-observer": "^2.5.0",
    "@uupaa/dynamic-import-polyfill": "^1.0.2",
    "autoprefixer": "^9.5.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.2.1",
    "deasync": "^0.1.28",
    "dynamic-import-polyfill": "^0.1.1",
    "objectFitPolyfill": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-css-url-loader": "^1.0.5",
    "parcel-plugin-goodie-bag": "^2.0.0",
    "parcel-plugin-inlinesvg": "^0.1.0",
    "parcel-plugin-purifycss": "^1.0.5",
    "parcel-plugin-url-loader": "^1.2.6",
    "parcel-plugin-watch-reload": "^1.0.3",
    "sass": "^1.25.0",
    "swiper": "^8.1.4"
  },
  "scripts": {
    "watch": "parcel watch ./assets/scripts/js/main.js --public-url ./",
    "dev": "parcel ./assets/scripts/js/main.js",
    "build": "parcel build ./assets/scripts/js/main.js --public-url ./",
    "vipbuild": "parcel build ./assets/scripts/js/storeLocator.js --public-url ./ --out-dir ./assets/css/ --out-file store-finder.js",
    "vip": "parcel watch ./assets/scripts/js/storeLocator.js --public-url ./ --out-dir ./assets/css/ --out-file store-finder.js"
  },
  "jshintConfig": {
    "globals": {
      "jQuery": true
    }
  },
  "watchreload": {
    "files": "**/*.php"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.3.5",
    "@fancyapps/ui": "^4.0.8",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "bindings": "^1.5.0",
    "carousel-ticker": "^1.0.0",
    "countup.js": "^2.0.7",
    "custom-select": "^1.1.15",
    "foundation-sites": "^6.6.3",
    "gsap": "^3.6.1",
    "html2pdf.js": "^0.10.1",
    "iframe-resizer": "^4.3.2",
    "jquery": "^3.3.1",
    "jspdf": "^2.5.1",
    "menuspy": "^1.3.0",
    "motion-ui": "^1.2.2",
    "node-gyp": "^8.4.1",
    "nodesass": "^0.0.2-security",
    "parcel": "^1.12.4",
    "scrollmagic": "^2.0.8",
    "scrollreveal": "^4.0.9"
  }
}
