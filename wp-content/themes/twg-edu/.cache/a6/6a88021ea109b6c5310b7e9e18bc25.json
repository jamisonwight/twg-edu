       const colorEnd = obj.dataset.transitionColorEnd\n        const backgroundLighten = obj.classList.contains('backgroundLighten')\n        const animSection = obj.classList.contains('animSection')\n\n        if (animSection) {\n            let tl = gsap.timeline({\n                // yes, we can add it to an entire timeline!\n                scrollTrigger: {\n                    trigger: obj,\n                    start: (animSection) ? 'top-=200px top+=200px' : 'top-=300px top+=300px',\n                    end:  (animSection) ? '+=500px' : '+=700px',\n                    scrub: true,\n                }\n            })\n\n            tl.fromTo(obj,\n                {background: colorStart},\n                {background: ((backgroundLighten) ? hexToRgbA(colorEnd) : colorEnd)})\n        } else {\n            gsap.to(document.body,\n            {\n                scrollTrigger: {\n                    trigger: obj,\n                    start: (animSection) ? 'top-=200px top+=200px' : 'top-=300px top+=300px',\n                    end:  (animSection) ? '+=500px' : '+=700px',\n                    scrub: true,\n                },\n                background: ((backgroundLighten) ? hexToRgbA(colorEnd) : colorEnd),\n                duration: 1000\n            })\n        }\n    })\n}\n\nfunction hexToRgbA(hex){\n    var c;\n    if(/^#([A-Fa-f0-9]{3}){1,2}$/.test(hex)){\n        c= hex.substring(1).split('');\n        if(c.length== 3){\n            c= [c[0], c[0], c[1], c[1], c[2], c[2]];\n        }\n        c= '0x'+c.join('');\n        return 'rgba('+[(c>>16)&255, (c>>8)&255, c&255].join(',')+', 0.3)';\n    }\n    throw new Error('Bad Hex');\n}"},"lineCount":null}},"error":null,"hash":"54b7fa8ab206c26ed11a30516fd4a421","cacheData":{"env":{}}}