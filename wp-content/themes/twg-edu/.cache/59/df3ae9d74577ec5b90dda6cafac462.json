         }\n        })\n\n        const slidesRight = new swiper(obj.querySelector('.slides-right-container'), {\n            direction: 'horizontal',\n            loop: true,\n            autoplay: false,\n            centeredSlides: true,\n            allowTouchMove: false,\n        })\n\n        slidesLeft.on('slideChangeTransitionEnd', () => {\n            slidesRight.slideToLoop(slidesLeft.realIndex)\n        })\n\n        slidesLeft.autoplay.stop()\n\n        ScrollReveal().reveal(obj, {\n            afterReveal: (el) => {\n                slidesLeft.autoplay.start()\n            },\n            viewFactor: 0,\n        })\n    })\n\n}\n"},"lineCount":null}},"error":null,"hash":"d6091e0eccc4dd7d2e52125b557a3f6c","cacheData":{"env":{}}}